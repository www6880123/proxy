<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.hj.proxy.frontdesk.dao.AccountDao">
    <select id="loginAccount" parameterType="com.hj.proxy.frontdesk.entity.Account">
        SELECT * FROM `account` WHERE username={username},`password`={password}
    </select>

</mapper>